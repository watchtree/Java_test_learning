/**
 * 
 * @author tree
 *
 **/

public class User {
	int id;
	String name;
	String pwd;
	
	public User(){
		
	}
	
	public User(int id, String name){
		super();//构造方法的第一句总是super，不写会默认补全
		this.id = id;
		this.name = name;	
	}
	
	public User(int id, String name, String pwd){
		this.id = id;
		this.name = name;
		this.pwd = pwd;
	}
	
	public static void main(String[] args) {
		User u1 = new User();
		User u2 = new User(101, "wtt");
		User u3 = new User(100, "tree", "12345");
	}
	
}
