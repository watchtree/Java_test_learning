import java.awt.*;
import javax.swing.*;

public class BallGame extends JFrame{
	
	//窗口加载
	void launchFrame(){
		setSize(300,300);
		setLocation(50,50);
		setVisible(true);
		
	}

	//main方法时程序执行的入口
	public static void main(String[] args){
		System.out.println("hello world");
		BallGame game = new BallGame();
		game.launchFrame();
	}
}
