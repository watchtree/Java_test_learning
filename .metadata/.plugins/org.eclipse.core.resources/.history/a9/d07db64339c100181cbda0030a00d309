public class TestWhileQiantao {
	public static void main(String[] args) {
		for(int i=1;i<=5;i++){
			for(int j=1;j<=5;j++){
				System.out.print(j + "\t");
			}
			System.out.println("");//´òÓ¡²¢»»ÐÐ
		}
		
		System.out.println("############");
		
		for(int p=1;p<=9;p++){
			for(int q=1;q<=p;q++){
				System.out.print(p+"*"+q+"="+p*q+"\t");
			}
			System.out.println("");
		}
		System.out.println("############");
		
		int sum1=0;
		int sum2=0;
		int i=1;
		while(i<=100){
			if(i%2==1){
				sum1=sum1+i;
			}else{
				sum2=sum2+i;
			}
		}
		System.out.println(sum1);
	}
}
