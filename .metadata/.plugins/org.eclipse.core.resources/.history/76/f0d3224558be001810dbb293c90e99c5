public class TestOperator01 {
	public static void main(String[] args) {
		byte a = 1;
		int b = 2;
		long b2 = 3;
		//byte c = a+b;
		//int c2 = b2+b;
		float f1 = 3.14F;
		double d = b+b2;
		//表述范围较大可以自动转换
		
		//float d2 = f1+6.2;
		
		System.out.println(-9%5);
		
		//测试自增和自减
		int a = 3;
		int b = a++;
		System.out.println("a=" + a + "\nb=" +b;)
	}
	
}
