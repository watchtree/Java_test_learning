package cn.bjsxt.array2;

public class Matrix {
	
	public static void print(int[][] c){
		/**
		 * ´òÓ¡Ö¸¶¨¾ØÕó
		 */
		for(int i=0;i<c.length;i++){
			for(int j=0;j<c.length;j++){
				System.out.print(c[i][j]+"\t");  
			}
			System.out.println("");
		}
	}
	public static void main(String[] args) {
		int[][] a={
				{1,3},
				{2,4},
		};
		int[][] b={
				{3,4},{5,6},
		};
		int[][] c = new int[2][2];
		
		Matrix.print(c);
		

	}
}
