
public class TestPrimitiveDataType {
	public static void main(String[] args){
		//测试整形变量
		int a = 15;
		int b = 015; //八进制
		int c = 0x15; // 十六进制
		int d = 0b1101; //二进制
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
		
		byte age = 30;
		short salary = 30000;
		int population = 3000000000;
		long globalPopution = 740000000;
		
		
	}
}
