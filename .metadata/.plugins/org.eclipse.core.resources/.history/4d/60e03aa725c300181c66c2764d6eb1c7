public class SxtStu {
	//属性，成员变量 
	int id;
	String name;
	int age;
	
	Computer comp;
	
	void study(){
		System.out.println("study" + comp.brand);
	}
	
	void play(){
		System.out.println("play");
		
	}
	
	//系统自动创建类下面的对象，构造方法，无参数的构造方法可以省略
	SxtStu(){
		
	}
	
	public static void main(String[] args) {
		SxtStu stu = new SxtStu();//创建一个对象
		stu.id=1001;
		stu.name="wtt";
		stu.age=18;
		
		Computer c1 = new Computer();
//		c1.brand = "联想";
//		stu.comp = c1;
		stu.comp.brand="联想";
		stu.study();
		stu.play();
	}
}

class Computer {
	String brand;
	
}
