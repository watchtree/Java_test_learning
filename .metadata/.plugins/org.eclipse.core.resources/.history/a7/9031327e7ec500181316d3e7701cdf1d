public class User2 {
	int id;
	String name;
	String pwd;
	static String company = "华电";
	
	public User2(int id, String name){
		this.id = id;
		this.name = name;
	}
	
	public void login(){
		System.out.println("登陆" + name);
		
	}
	
	public static void printCompany(){
		//login();
		System.out.println(company);
	}
	
	public static void main(String[] args) {
		User2 u = new User2(101,"wtt");
		User2.printCompany();
		User2.company = "北京";
		User2.printCompany();
	}
}
