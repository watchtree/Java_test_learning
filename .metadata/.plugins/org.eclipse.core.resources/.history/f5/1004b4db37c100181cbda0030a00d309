public class TestWhileQiantao {
	public static void main(String[] args) {
		for(int i=1;i<=5;i++){
			for(int j=1;j<=5;j++){
				System.out.print(j + "\t");
			}
			System.out.println("");//´òÓ¡²¢»»ÐÐ
		}
		
		System.out.println("############");
		
		for(int p=1;p<=9;p++){
			for(int q=1;q<=p;q++){
				System.out.print(p+"*"+q+"="+p*q);
			}
			System.out.println("");
		}
	}
}
